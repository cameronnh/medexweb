@model medexnet.Models.UserModel

@{
    Layout = "~/Views/Shared/DoctorLayout.cshtml";
}

<div class="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title m-b-0">Your Patients</h5>
                    </div>
                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">First Name</th>
                                <th scope="col">Last Name</th>
                                @*<th scope="col">OTHER</th>
                                    <th scope="col">OTHER</th>*@
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (medexnet.Models.UserModel item in Model.myPatients)
                            {
                            <tr>
                                <td>@Html.DisplayFor(modelItem => item.fName)</td>
                                <td>@Html.DisplayFor(modelItem => item.lName)</td>
                            </tr>
                            }
                        @*<tr>
                            <th scope="row">1</th>
                            <td>MIKE</td>
                            <td>HUNT</td>
                        </tr>*@
                        </tbody>
                    </table>
                </div>

            </div>

        </div>
    </div>

</div>